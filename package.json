{
  "name": "some-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "node tools/run.js",
    "build": "NODE_ENV=production node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "test:lint": "eslint src test tools postcss.config.js webpack.config.js",
    "test:unit": "jest",
    "test:dependencies": "snyk auth $SNYK_TOKEN && snyk test && snyk monitor"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "apollo-link-retry": "^2.1.1",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.8",
    "graphql": "^14.0.2",
    "graphql-tag": "2.10.0",
    "immutability-helper": "2.8.1",
    "jump.js": "^1.0.2",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-apollo": "2.2.4",
    "react-dom": "16.6.3",
    "react-helmet": "5.2.0",
    "react-hot-loader": "^4.1.1",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "assets-webpack-plugin": "3.9.7",
    "autoprefixer": "9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.0",
    "browser-sync": "2.26.0",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "2.0.0",
    "cssnano": "^4.1.7",
    "ejs": "2.6.1",
    "enzyme": "3.7.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "front-matter": "3.0.0",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "path-to-regexp": "2.4.0",
    "pixrem": "4.0.1",
    "pleeease-filters": "4.0.0",
    "postcss": "7.0.5",
    "postcss-calc": "6.0.2",
    "postcss-color-function": "4.0.1",
    "postcss-custom-media": "7.0.4",
    "postcss-custom-properties": "8.0.8",
    "postcss-custom-selectors": "5.1.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-media-minmax": "4.0.0",
    "postcss-nesting": "7.0.0",
    "postcss-selector-matches": "4.0.0",
    "postcss-selector-not": "4.0.0",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "1.1.2",
    "webp-loader": "0.4.0",
    "webpack": "4.20.2",
    "webpack-dev-middleware": "3.4.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "emotion",
      "react-hot-loader/babel",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "babel"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "test": true,
      "expect": true,
      "jest": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "react/jsx-filename-extension": "off",
      "max-len": "off",
      "babel/no-unused-expressions": 1
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.config.js"
        }
      }
    }
  }
}
